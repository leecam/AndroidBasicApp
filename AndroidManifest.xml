<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
      package="org.leecam.basic_app"
      android:versionCode="1"
      android:versionName="1.0">
    <uses-sdk android:minSdkVersion="14" android:targetSdkVersion="21" />
    <permission android:name="ServicePerm" android:protectionLevel="signature" />
    <application android:label="Basic App"
                 android:debuggable="true">
        <activity android:name=".BasicAppActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <service android:name=".BasicAppService"
                 android:process=":BasicAppService"
                 android:isolatedProcess="false"
                 android:permission="ServicePerm"
                 android:exported="false" />
    </application>
</manifest>
